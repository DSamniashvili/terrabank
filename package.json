{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-14-pro": "react-native run-ios --simulator='iPhone 14 pro'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "test": "jest",
    "type-check": "tsc --noemit --skipLibCheck",
    "test:related": "jest --watchAll=false --findRelatedTests --forceExit",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "reinstall": "rm -rf node_modules && yarn install && yarn reinstall:pods",
    "reinstall:pods": "cd ios && rm -rf Pods && pod install",
    "verify": "yarn type-check && yarn prettier && yarn lint",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn verify"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "4.5.1",
    "@react-native-community/blur": "^4.3.2",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/elements": "^1.3.21",
    "@react-navigation/material-top-tabs": "6.6.4",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "async-mutex": "0.4.0",
    "axios": "0.19.2",
    "dayjs": "^1.11.10",
    "i18next": "^23.2.3",
    "react": "18.2.0",
    "react-hook-form": "7.47.0",
    "react-i18next": "^13.0.1",
    "react-native": "0.72.1",
    "react-native-biometrics": "2.1.4",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-keychain": "^8.1.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.10.0",
    "react-native-pager-view": "^6.2.2",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^3.22.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-svg": "13.14.0",
    "react-native-tab-view": "3.5.2",
    "react-redux": "^8.1.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.7",
    "@react-navigation/devtools": "^6.0.19",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.5",
    "@types/metro-config": "^0.76.3",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.19.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-native-flipper": "0.125.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16"
  }
}
